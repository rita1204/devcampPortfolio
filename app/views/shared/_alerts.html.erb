<% if flash[:alert]%>
  <%= js add_gritter(flash[:alert],title: "TAKUTO MORI PORTFOLIO",sticky:false) %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error],title: "TAKUTO MORI PORTFOLIO",sticky:false) %>
<% else %>
  <%= js add_gritter(flash[:notice],title: "TAKUTO MORI PORTFOLIO",sticky:false) %>
<% end %>